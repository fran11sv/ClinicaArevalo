	<queryString>
		<![CDATA[SELECT
     Receta.`Num_Receta` AS NumReceta,
     Receta.`id_Consulta` AS RidConsulta,
     Receta.`Indicaciones` AS Indicaciones,
     Paciente.`id_Paciente` AS Paciente,
     Paciente.`Nombres` AS PNombres,
     Paciente.`Apellidos` AS PApellidos,
     Consulta.`id_Consulta` AS CidConsulta,
     Consulta.`id_Paciente` AS CidPaciente,
     Consulta.`id_usuario` AS Cidusuario,
     Consulta.`Fecha_Consulta` AS CFechaConsulta,
     Paciente.`Fecha_Nacimiento` AS Edad
FROM
     `Paciente` Paciente INNER JOIN `Consulta` Consulta ON Paciente.`id_Paciente` = Consulta.`id_Paciente`
     INNER JOIN `Receta` Receta ON Consulta.`id_Consulta` = Receta.`id_Consulta`
WHERE
     Receta.`Num_Receta` = $P{numReceta}]]>
	</queryString>
