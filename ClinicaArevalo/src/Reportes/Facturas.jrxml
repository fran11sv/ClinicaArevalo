		<queryString language="SQL">
			<![CDATA[SELECT
     Producto.`Nombre` AS 'Producto',
     Producto.`Detalle` AS 'Descripción',
     Producto.`Precio` AS 'Precio unitario'
FROM
     `Producto` Producto]]>
		</queryString>
		<field name="Producto" class="java.lang.String"/>
		<field name="Precio unitario" class="java.math.BigDecimal"/>
		<variable name="Sumas" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{Precio unitario}]]></variableExpression>
		</variable>
