			<variableExpression><![CDATA[$F{Precio unitario}]]></variableExpression>
		</variable>
	</subDataset>
	<subDataset name="DSFactura" uuid="d440898e-ded0-4e46-a867-f2cb3f369a34">
		<queryString language="SQL">
			<![CDATA[SELECT
     Factura.`Fecha_Factura` AS 'Fecha'
FROM
     `Factura` Factura]]>
		</queryString>
		<field name="Fecha" class="java.sql.Date"/>
		<group name="Fecha">
			<groupExpression><![CDATA[$F{Fecha}]]></groupExpression>
		</group>
	</subDataset>
	<parameter name="numFactura" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     Factura.`Num_Factura` AS N°_Factura,
     Factura.`Nombre_Cliente` AS Cliente,
     Factura.`Fecha_Factura` AS Fecha,
     Factura.`Direccion_cliente` AS Direccion,
     Producto.`Precio` AS Producto_Precio,
     Factura.`num_letras` AS Factura_num_letras,
     Factura.`Total` AS Factura_Total
FROM
     `Factura` Factura,
     `Producto` Producto
WHERE
     Factura.`Num_Factura` = $P{numFactura}]]>
	</queryString>
	<field name="N°_Factura" class="java.lang.Integer"/>
	<field name="Cliente" class="java.lang.String"/>
